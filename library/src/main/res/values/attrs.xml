<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="SwitchButton">
		<attr name="frameDrawable" format="reference|color"/>
		<attr name="stateDrawable" format="reference|color"/>
		<attr name="stateMaskDrawable" format="reference|color"/>
		<attr name="sliderDrawable" format="reference|color"/>
		<attr name="withTextInterval" format="dimension"/>
	</declare-styleable>

	<declare-styleable name="PagerSlidingTabStrip">
		<attr name="pstsIndicatorColor" format="color"/>
		<attr name="pstsUnderlineColor" format="color"/>
		<attr name="pstsDividerColor" format="color"/>
		<attr name="pstsDividerWidth" format="dimension"/>
		<attr name="pstsIndicatorHeight" format="dimension"/>
		<attr name="pstsUnderlineHeight" format="dimension"/>
		<attr name="pstsDividerPadding" format="dimension"/>
		<attr name="pstsTabPaddingLeftRight" format="dimension"/>
		<attr name="pstsScrollOffset" format="dimension"/>
		<attr name="pstsTabBackground" format="reference"/>
		<attr name="pstsPaddingMiddle" format="boolean"/>
		<attr name="pstsShouldExpand" format="boolean"/>
		<attr name="pstsProportion" format="integer"/>
		<attr name="pstsTextAllCaps" format="boolean"/>
		<attr name="pstsTabTextSize" format="dimension"/>
		<attr name="pstsTabTextColor" format="reference"/>
		<attr name="pstsSelectedTabTextColor" format="reference"/>
		<attr name="pstsTabTextStyle" format="reference">
			<flag name="normal" value="0x0"/>
			<flag name="bold" value="0x1"/>
			<flag name="italic" value="0x2"/>
		</attr>
		<attr name="pstsTabTextAllCaps" format="boolean"/>
		<attr name="pstsTabTextAlpha" format="integer"/>
		<attr name="pstsTabTextFontFamily" format="string"/>
	</declare-styleable>

	<declare-styleable name="CustomNodeLineView">
		<!-- 顶部节点外环半径 -->
		<attr name="topNodeOutSideRadius" format="dimension"/>
		<!-- 顶部节点内环半径 -->
		<attr name="topNodeInSideRadius" format="dimension"/>
		<!-- 其他节点半径 -->
		<attr name="otherNodeRadius" format="dimension"/>
		<!-- 节点（半径外）之间的距离 -->
		<attr name="nodeRadiusDistance" format="dimension"/>
		<!-- 顶部节点颜色 -->
		<attr name="topNodeColor" format="color"/>
		<!-- 其他节点颜色 -->
		<attr name="otherNodeColor" format="color"/>
		<!-- item顶部内边距边距 -->
		<attr name="itemPaddingTop" format="dimension"/>
		<!-- 节点个数 -->
		<attr name="nodeCount" format="integer"/>
		<!-- View的宽度 -->
		<attr name="viewWidth" format="dimension"/>
	</declare-styleable>


	<declare-styleable name="SlidingLayout">
		<attr name="background_view" format="reference"/>
		<attr name="sliding_mode" format="enum">
			<enum name="both" value="0"/>
			<enum name="top" value="1"/>
			<enum name="bottom" value="2"/>
		</attr>
		<attr name="sliding_pointer_mode" format="enum">
			<enum name="one" value="0"/>
			<enum name="more" value="1"/>
		</attr>
		<attr name="top_max" format="dimension"/>
	</declare-styleable>

	<declare-styleable name="pickerview">
		<attr name="pickerview_gravity">
			<enum name="center" value="17"/>
			<enum name="left" value="3"/>
			<enum name="right" value="5"/>
		</attr>
		<attr name="pickerview_textSize" format="dimension"/>
		<attr name="pickerview_textColorOut" format="color"/>
		<attr name="pickerview_textColorCenter" format="color"/>
		<attr name="pickerview_dividerColor" format="color"/>
	</declare-styleable>

	<declare-styleable name="SeekBarIndicated">
		<attr name="seekbar_marginLeft" format="dimension"/>
		<attr name="seekbar_marginTop" format="dimension"/>
		<attr name="seekbar_marginRight" format="dimension"/>
		<attr name="seekbar_marginBottom" format="dimension"/>
		<attr name="seekbar_min" format="integer"/>
		<attr name="seekbar_max" format="integer"/>
		<attr name="seekbar_thumb" format="reference"/>
		<attr name="seekbar_progressDrawable" format="reference"/>
		<attr name="seekbar_sideMarks" format="boolean"/>
		<attr name="indicator_src" format="reference"/>
		<attr name="indicator_paddingLeft" format="dimension"/>
		<attr name="indicator_paddingTop" format="dimension"/>
		<attr name="indicator_paddingRight" format="dimension"/>
		<attr name="indicator_paddingBottom" format="dimension"/>
		<attr name="indicator_text" format="reference"/>
		<attr name="indicator_textStyle" format="enum">
			<enum name="normal" value="0"/>
			<enum name="bold" value="1"/>
			<enum name="italic" value="2"/>
		</attr>
		<attr name="indicator_textColor" format="color"/>
		<attr name="indicator_textCenterHorizontal" format="boolean"/>
		<attr name="indicator_textCenterVertical" format="boolean"/>
		<attr name="indicator_textMarginLeft" format="dimension"/>
		<attr name="indicator_textMarginTop" format="dimension"/>
		<attr name="indicator_textMarginRight" format="dimension"/>
		<attr name="indicator_textMarginBottom" format="dimension"/>
	</declare-styleable>

	<declare-styleable name="RoundProgressBar">
		<attr name="roundColor" format="color"/>
		<attr name="roundProgressColor" format="color"/>
		<attr name="roundWidth" format="dimension"/>
		<attr name="textColor" format="color"/>
		<attr name="textSize" format="dimension"/>
		<attr name="max" format="integer"/>
		<attr name="textIsDisplayable" format="boolean"/>
		<attr name="style">
			<enum name="STROKE" value="0"/>
			<enum name="FILL" value="1"/>
		</attr>
	</declare-styleable>

</resources>